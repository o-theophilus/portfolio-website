{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
  "crons": [{
    "path": "/cron",
    "schedule": "*/10 * * * *"
    }]
}